<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Symfonico\ConsorcioBundle\Entity\CuentacorrienteConceptos" table="cuentacorriente_conceptos">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="concepto" type="string" column="concepto" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="debe" type="boolean" column="debe" nullable="false"/>
    <field name="haber" type="boolean" column="haber" nullable="false"/>
  </entity>
</doctrine-mapping>
